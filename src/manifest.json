{
  "manifest_version": 3,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "default_locale": "en",
  "version": "1.5.6",
  "author": "dev@conceptualspace.net",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "*://app.plex.tv/*",
    "*://localhost/*",
    "*://127.0.0.1/*"
  ],
  "optional_host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "browser_style": true,
    "default_title": "Configure Enhance-O-Tron for Plex",
    "default_popup": "options.html"
  },
  "icons": {
    "16": "img/icon16.png",
    "32": "img/icon32.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["img/icon219.svg", "img/compress.svg"],
      "matches": ["<all_urls>"]
    }
  ]
}
